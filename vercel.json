{
  "version": 2,
  "builds": [
    {
      "src": "frontend/streamlit_app.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "frontend/streamlit_app.py"
    }
  ],
  "env": {
    "PYTHONPATH": "frontend"
  }
} 