{
  "version": 2,
  "builds": [
    {
      "src": "frontend/streamlit_app.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9",
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "frontend/streamlit_app.py"
    }
  ],
  "env": {
    "PYTHONPATH": "frontend"
  },
  "functions": {
    "frontend/streamlit_app.py": {
      "memory": 3008,
      "maxDuration": 10
    }
  }
} 